class Main{

/* Sumatoria */

float sumatoria(float b){
	float total;
	float i;
	float res;
        total = 0.0;
        i = b;
        
	while(i > 0.0  || i > 0.0 )   {
		if(i > 0.0){
			total = total + i;
                }
		i -= 1.0;
	} /* fin while */
	res = total;
	return res;

}

void printf(float f) extern;

void main(){
     float f;
     float a;
     a = 8.0;
     f = sumatoria(a);
     printf(f);
     return;
}

}
