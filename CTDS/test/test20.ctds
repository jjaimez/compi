class Main {

        void main() {
                int id2;
                float id1;
                id2 = 2;
                id2 += id2;
                id2 -= id2 + id2 * id2 / id2;                
                id1 = 1.3;
                id1 += id1 * id1;
                id1 -= id1;                
	}
}
