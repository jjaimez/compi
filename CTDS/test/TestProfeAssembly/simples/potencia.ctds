class Main{

    void print_float(float x) extern;
    void print_int(int i) extern;
    void print_orig() extern;
    void print_reverse() extern;
    void print_ln()extern;



    float potencia(float num,int pot){
        int i;
        float res;
        i = 2;
        res = num;
        if(pot == 0){
            return 1.0;
        }
        for i=2,pot+1 {
            res = res * num;
        }
        
        return res;
    }
    
    void main(){
        print_float( potencia(4.0,0));
        print_float( potencia(4.0,1));
        print_float( potencia(4.0,2));
        print_float( potencia(4.0,3));
        print_float( potencia(5.0,2));
        print_float( potencia(5.0,5));
        print_float( potencia(2.0,16));
    }
    

}
