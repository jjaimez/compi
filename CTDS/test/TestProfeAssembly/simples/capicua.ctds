


class Main{
    int length;
    int num[10];
   
    void print_float(float x) extern;
    void print_int(int i) extern;

    int toInt(boolean b){
        if(b){
           return 1;
        }
        else{
           return 0;
        }
    }


    
    boolean isCapicua(){
        int i;
        for i=0,i == length/2 {
            if(num[i] != num[(length-1) -i]){
                return false;
            }        
        }
        return true;
    }
    
    void main(){
        int w;
        w = 8;
        length = 5;
        num[0] = 1;
        num[1] = 0;
        num[2] = w;
        num[3] = 0;
        num[4] = 1;        
        print_int(toInt(isCapicua()));
        
        w=2;
        num[0] = w;
        num[1] = 1;
        num[2] = 7;
        num[3] = 1;
        num[4] = w;        
        print_int(toInt(isCapicua()));
        
        return;
    }

}
